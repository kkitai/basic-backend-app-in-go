    "host": "localhost:3000",
    "paths": {
        "/telephones": {
            "get": {
                "description": "returns all telephone informations",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "telephone"
                ],
                "summary": "list telephone informations",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/telephones/{number}": {
            "get": {
                "description": "returns one telephone information by telephone number",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "telephone"
                ],
                "summary": "get a telephone information",
                "parameters": [
                    {
                        "minLength": 11,
                        "type": "string",
                        "description": "telephone number",
                        "name": "number",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}